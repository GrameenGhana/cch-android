<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        android:layout_width="match_parent"
        android:background="@color/Curry"
        android:orientation="vertical"
        android:padding="10dp" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="0.08"
            android:background="@color/White"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/TextView01"
                android:layout_width="wrap_content"
                android:layout_height="38dp"
                android:text="@string/str_care_new_born"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@color/Blue" />
        </LinearLayout>

        <TextView
            android:id="@+id/TextView03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/White"
            android:text="@string/str_help_client_compli_ready_plan"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="@color/TextBrown" />

        <LinearLayout
            android:layout_width="286dp"
            android:layout_height="wrap_content"
            android:background="@color/TakeActionCurry"
            android:divider="@drawable/divider"
            android:orientation="vertical"
            android:showDividers="middle" >
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/White"
            android:orientation="vertical"
            android:padding="5dp" >

            <TextView
                android:id="@+id/TextView0790"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingBottom="5dp"
                android:text="@string/str_register_for_nhis"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <TextView
                android:id="@+id/TextView02"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingBottom="5dp"
                android:text="@string/str_need_ear_piercing"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <TextView
                android:id="@+id/TextView09"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingBottom="5dp"
                android:text="@string/str_sickle_cell_screening"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <TextView
                android:id="@+id/TextView06"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingBottom="5dp"
                android:text="@string/str_importance_cord_care"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <TextView
                android:id="@+id/TextView04"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingBottom="5dp"
                android:text="@string/str_counsel_pregnant_woman_after_baby_born"
                android:textAppearance="?android:attr/textAppearanceSmall" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/cab_background_bottom_example"
            android:orientation="vertical"
            android:padding="5dp" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <ImageView
                    android:id="@+id/ImageView05"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/breast_feeding_family_planning" />

                <TextView
                    android:id="@+id/TextView13"
                    android:layout_width="232dp"
                    android:layout_height="match_parent"
                    android:text="@string/str_breast_feed_frequently_10_24hr"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:textColor="@color/TextBrown" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <TextView
                    android:id="@+id/TextView12"
                    android:layout_width="232dp"
                    android:layout_height="match_parent"
                    android:text="@string/str_good_hygience_hand_washing"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:textColor="@color/TextBrown" />

                <ImageView
                    android:id="@+id/ImageView04"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/express_milk4" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <ImageView
                    android:id="@+id/ImageView03"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/rest_take_fluid" />

                <TextView
                    android:id="@+id/TextView11"
                    android:layout_width="232dp"
                    android:layout_height="match_parent"
                    android:text="@string/str_need_rest_drink_plenty_fluid"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:textColor="@color/TextBrown" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <TextView
                    android:id="@+id/TextView10"
                    android:layout_width="232dp"
                    android:layout_height="match_parent"
                    android:text="@string/str_importance_sleep_itn_every_night"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:textColor="@color/TextBrown" />

                <ImageView
                    android:id="@+id/ImageView02"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/sleep_itn_net" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <ImageView
                    android:id="@+id/ImageView01"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/nurse_advice_drugs" />

                <TextView
                    android:id="@+id/TextView08"
                    android:layout_width="232dp"
                    android:layout_height="match_parent"
                    android:text="@string/str_iron_folic_acid_suppliment"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:textColor="@color/TextBrown" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <TextView
                    android:id="@+id/TextView05"
                    android:layout_width="232dp"
                    android:layout_height="match_parent"
                    android:text="@string/str_keep_babdy_warm"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:textColor="@color/TextBrown" />

                <ImageView
                    android:id="@+id/imageView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/baby_keep_warm" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</ScrollView>